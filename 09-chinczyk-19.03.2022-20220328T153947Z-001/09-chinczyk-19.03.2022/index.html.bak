<div id ="container">
  <div id ="animate"></div>
</div>

<script>
  let pos = 0;
  const elem = document.getElementById("animate");

function myMove() 
{
	let id = null;
	pos = 0;
	clearInterval(id);
	id = setInterval(frame, 5);
	function frame() 
	{
		if (pos == 350) 
		{
		clearInterval(id);
		} 	
		else 
		{
		pos++; 
		elem.style.top = pos + "px"; 
		//elem.style.left = pos + "px";//
		}
	}
}

function resett()
{
	pos = 0;
	elem.style.top = pos  + "px";
	elem.style.left = pos  + "px";
	elem.style.right = pos  + "px";
}

function rightt()
{
	pos = pos + 10;
	elem.style.left = pos  + "px";
}
